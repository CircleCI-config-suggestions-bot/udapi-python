#!/usr/bin/env python

import sys
import logging

from udapi.core.run import Run

# Initialize logging.
logging.basicConfig(format='%(asctime)-15s [%(levelname)6s] %(funcName)s - %(message)s', level=logging.INFO)

# Process and provide the scenario.
if __name__ == "__main__":
    command_line_scenario = sys.argv[1:]

    runner = Run(command_line_scenario)
    runner.execute()
